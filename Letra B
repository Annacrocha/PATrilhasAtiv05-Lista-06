using System;

namespace ATIVIDADE_4_
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Calculo da diferença entre duas datas\n");
			
			DateTime data1 = new DateTime (2007, 05, 26);
			DateTime data2 = new DateTime (2025, 02, 05);
			
			TimeSpan diferenca = data2 - data1;
			
			Console.WriteLine("Diferença entre dias: " + diferenca.TotalDays);
				
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
